general {
	interval = 5
	colors = true
	color_good = "#a3be8c"
	color_degraded = "#d08770"
	color_bad = "#bf616a"
}

order += "volume master"
order += "tztime local"


volume master {
	format = " %volume "
        format_muted = " %volume "
        device = "default"
        mixer = "Master"
	mixer_idx = 0
}



tztime local {
	format = " %a %Y-%m-%d %l:%M %p"
}
